  "scripts": {
    "preinstall": "bash eas-build-pre-install.sh",
    "dev": "NODE_ENV=development expo start --dev-client",
    "ios:dev": "expo run:ios --port 8081 --scheme 'pocketpt-development' --configuration Debug",
    "submit": "eas submit -p ios --latest",
    "test": "jest",
    "util:bump-version": "npx react-native-version --never-amend",
    "util:sort-pkg": "npx sort-package-json",
    "util:adb:map": "adb reverse tcp:5000 tcp:5000",
