{
  "name": "pocketpt",
  "version": "0.2.0",
  "scripts": {
    "preinstall": "bash eas-build-pre-install.sh",
    "start": "expo start --dev-client",
    "web": "expo start --web",
    "android:dev": "API_SERVER=0 expo start --android",
    "android:staging": "API_SERVER=1 expo start --android ",
    "android:prod": "API_SERVER=2 expo start --android ",
    "android:device": "expo start --android --tunnel --clear",
    "ios:dev": "API_SERVER=0 expo start --ios",
    "ios:staging": "API_SERVER=1 expo start --ios ",
    "ios:prod": "API_SERVER=2 expo start --ios ",
    "build:android:local": "EAS_LOCAL_BUILD_SKIP_CLEANUP=1 eas build -p android --local",
    "build:ios:local": "EAS_LOCAL_BUILD_SKIP_CLEANUP=1 eas build -p ios --local",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "submit": "eas submit -p ios --latest",
    "test": "jest",
    "util:bump-version": "npx react-native-version --never-amend",
    "util:sort-pkg": "npx sort-package-json",
    "util:adb:map": "adb reverse tcp:5000 tcp:5000",
    "invite-ios:bcdevlucas": "npx uri-scheme open pocketpt://accept-invitation/6190693aa0c0e20021fa2324 --ios",
    "invite-ios:armos51": "npx uri-scheme open pocketpt://accept-invitation/61907743a0c0e20021fa232f --ios",
    "invite-ios:test": "npx uri-scheme open pocketpt://accept-invitation/637147fefb4dfea595826d8d --ios",
    "invite-android:bcdevlucas": "npx uri-scheme open pocketpt://accept-invitation/6190693aa0c0e20021fa2324 --android",
    "invite-android:armos51": "npx uri-scheme open pocketpt://accept-invitation/61907743a0c0e20021fa232f --android",
    "invite-android:test": "npx uri-scheme open pocketpt://accept-invitation/637147fefb4dfea595826d8d --android",
    "expo-invite-ios:bcdevlucas": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/6190693aa0c0e20021fa2324 --ios",
    "expo-invite-ios:armos51": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/61907743a0c0e20021fa232f --ios",
    "expo-invite-ios:test": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/637147fefb4dfea595826d8d --ios",
    "expo-invite-android:bcdevlucas": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/6190693aa0c0e20021fa2324 --android",
    "expo-invite-android:armos51": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/61907743a0c0e20021fa232f --android",
    "expo-invite-android:test": "npx uri-scheme open exp://127.0.0.1:19000/--/accept-invitation/637147fefb4dfea595826d8d --android",
    "ios": "expo run:ios"
  },
